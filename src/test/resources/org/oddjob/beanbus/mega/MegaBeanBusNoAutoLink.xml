<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<oddjob>
    <job>
        <bean class="org.oddjob.beanbus.mega.MegaBeanBus" id="bus" noAutoLink="true">
            <parts>
                <bus:iterable-driver xmlns:bus="oddjob:beanbus">
                    <beans>
                        <list>
                            <values>
                                <value value="Apple"/>
                                <value value="Orange"/>
                                <value value="Pear"/>
                            </values>
                        </list>
                    </beans>
                    <to>
                        <value value="${capture}"/>
                    </to>
                </bus:iterable-driver>
                <bean class="org.oddjob.beanbus.mega.MegaBeanBusTest$OutboundCapture" id="capture">
                    <outbound>
                        <value value="${our-list}"/>
                    </outbound>
                </bean>
                <bus:capture id="list" xmlns:bus="oddjob:beanbus"/>
            </parts>
        </bean>
    </job>
</oddjob>
