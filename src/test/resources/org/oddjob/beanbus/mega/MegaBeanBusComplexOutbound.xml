<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<oddjob>
    <job>
        <bus:bus xmlns:bus="oddjob:beanbus" id="bus" noAutoLink="false">
            <of>
                <bus:driver xmlns:bus="oddjob:beanbus">
                    <values>
                        <list>
                            <values>
                                <value value="Apple"/>
                                <value value="Orange"/>
                                <value value="Pear"/>
                            </values>
                        </list>
                    </values>
                </bus:driver>
                <bean class="org.oddjob.beanbus.mega.MegaBeanBusTest$ComplicatedOutbound" id="capture">
                    <thing>
                        <is/>
                    </thing>
                </bean>
                <bus:collect id="list" xmlns:bus="oddjob:beanbus"/>
            </of>
        </bus:bus>
    </job>
</oddjob>
