<arooa:descriptor xmlns:arooa="http://rgordon.co.uk/oddjob/arooa"
                  namespace="oddjob:beanbus" prefix="bus">
    <components>
        <arooa:bean-def element='iterable-driver'
                        className='org.oddjob.beanbus.drivers.IterableBusDriver'/>
        <arooa:bean-def element='capture'
                        className='org.oddjob.beanbus.destinations.BeanCapture'/>
        <arooa:bean-def element='bean-copy'
                        className='org.oddjob.beanbus.destinations.BeanCopy'/>
        <arooa:bean-def element='bean-queue'
                        className='org.oddjob.beanbus.destinations.BeanQueue'/>
        <arooa:bean-def element='bean-sheet'
                        className='org.oddjob.beanbus.destinations.BeanSheet'/>
        <arooa:bean-def element='filter'
                        className='org.oddjob.beanbus.destinations.BeanFilter'/>
        <arooa:bean-def element='limit'
                        className='org.oddjob.beanbus.destinations.BeanLimit'/>
        <arooa:bean-def element='transformer'
                        className='org.oddjob.beanbus.destinations.BeanTransformer'/>
    </components>
</arooa:descriptor>